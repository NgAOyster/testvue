<?xml version="1.0"?>
<root>
  <LiquidOption>
    <title>
      <text>Liquid Level</text>
      <left>center</left>
    </title>
    <grid>
      <left>5%</left>
      <right>5%</right>
      <containLabel>true</containLabel>
    </grid>
    <tooltip>
      <trigger>axis</trigger>
      <axisPointer>
        <type>shadow</type>
      </axisPointer>
    </tooltip>
    <xAxis>
      <type>category</type>
      <axisTick>
        <alignWithLabel>true</alignWithLabel>
      </axisTick>
    </xAxis>
    <yAxis>
      <type>value</type>
      <axisLabel>
        <formatter>{value} %</formatter>
      </axisLabel>
      <max>100</max>
    </yAxis>
    <series>
      <name>liquid level</name>
      <type>bar</type>
      <barWidth>30%</barWidth>
      <showBackground>true</showBackground>
    </series>
    [object Object]
  </LiquidOption>
  <TempOption>
    <title>
      <text>Temperature</text>
      <left>center</left>
    </title>
    <legend>
      <top>10%</top>
    </legend>
    <grid>
      <left>5%</left>
      <right>5%</right>
      <top>30%</top>
      <containLabel>true</containLabel>
    </grid>
    <tooltip>
      <trigger>axis</trigger>
    </tooltip>
    <xAxis>
      <type>category</type>
      <axisTick>
        <alignWithLabel>true</alignWithLabel>
      </axisTick>
    </xAxis>
    <yAxis>
      <type>value</type>
      <nameLocation>start</nameLocation>
      <axisLabel>
        <formatter>{value} °C</formatter>
      </axisLabel>
    </yAxis>
    <series>
      <name>罐1</name>
      <type>line</type>
    </series>
    <series>
      <name>罐2</name>
      <type>line</type>
    </series>
    <series>
      <name>罐3</name>
      <type>line</type>
    </series>
    <series>
      <name>罐4</name>
      <type>line</type>
    </series>
    <series>
      <name>罐5</name>
      <type>line</type>
    </series>
    <series>
      <name>罐6</name>
      <type>line</type>
    </series>
    <series>
      <name>罐7</name>
      <type>line</type>
    </series>
    <dataZoom>
      <type>slider</type>
      <start>0</start>
      <end>100</end>
    </dataZoom>
    <dataZoom>
      <type>inside</type>
      <start>0</start>
      <end>100</end>
    </dataZoom>
    [object Object]
  </TempOption>
</root>