<?xml version="1.0"?>
<root>
  <LiquidOption>
    <title>
      <text>Liquid Level</text>
      <left>center</left>
    </title>
    <grid>
      <left>5%</left>
      <right>5%</right>
      <containLabel>true</containLabel>
    </grid>
    <tooltip>
      <trigger>axis</trigger>
      <axisPointer>
        <type>shadow</type>
      </axisPointer>
    </tooltip>
    <xAxis>
      <type>category</type>
      <data>罐1</data>
      <data>罐2</data>
      <data>罐3</data>
      <data>罐4</data>
      <data>罐5</data>
      <data>罐6</data>
      <data>罐7</data>
      <axisTick>
        <alignWithLabel>true</alignWithLabel>
      </axisTick>
    </xAxis>
    <yAxis>
      <type>value</type>
      <axisLabel>
        <formatter>{value} %</formatter>
      </axisLabel>
      <max>100</max>
    </yAxis>
    <series>
      <name>liquid level</name>
      <type>bar</type>
      <barWidth>30%</barWidth>
      <showBackground>true</showBackground>
      <data>16</data>
      <data>9</data>
      <data>24</data>
      <data>10</data>
      <data>21</data>
      <data>30</data>
      <data>10</data>
    </series>
    [object Object]
  </LiquidOption>
  <TempOption>
